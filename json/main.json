{
	"__topology_layer__": ["inter_meshnet","intra_subnet","outer_net_callbackNet"],

	"topology": {
		"inter-router": ["link-csma"],
		"node-router": {
			"intra-relay": ["link-p2p", "link-wifi"],
			"node-relay": {
				"intra-client": ["link-wifi"],
				"node-client": {}
			}
		}
	},
	
	"physical": {
		"link-csma": "template_csma",
		"node-router": {
			"node-number": 3,
			"node-config": [
				{
					"node-index":0,
					"link-p2p": "template_p2p",
					"link-wifi": "template_wifi_A",
					"node-relay": {
						"node-number": 1,
						"node-config": [
							{
								"node-index":0,
								"link-wifi": "template_wifi_B",
								"node-client": {
									"node-number": 5,
									"node-config": []
								}
							}
						]
					}
				},
				{
					"node-index":{"begin":1,"end":2},
					"relative":{
						"index":0,
						"update":{
							"node-relay/node-number":1
						},
						"append":{
							"node-relay/node-config":{
								"relative": {"index":0},
								"node-client/node-number": 4
							}
						}
					}
				}
			]
		}
	},

	"application": {},

	"template":{
		"template_csma": {
			"throughput": "1000Mbps",
			"delay": "2ms"
		},
		"template_p2p": {
			"throughput": "1000Mbps",
			"delay": "2ms"
		},
		"template_wifi_A": {
			"ssid": "wifi-A-%4%",
			"mobility": [0,5,0,0],
			"channel": 1,
			"bandwidth": 20,
			"standard": "802.11ac"
		},
		"template_wifi_B": {
			"ssid": "wifi-B-%4%",
			"mobility": [0,5,0,0],
			"channel": 1,
			"bandwidth": 20,
			"standard": "802.11ac"
		}
	}
}